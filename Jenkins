@Library('shared-libraries')
pipeline{
    agent any
    stages{
        stage ('stage1')
        {
            steps {
                 echo 'my first stage'
            }
        }
        stage ('stage2')
        {
            steps {
                 echo 'my Second stage'
            }
        }
        stage ('stage3 gitcheckout'){
            steps {
                script {
                    git branch: 'main',
                    credentialsId : 'github-token',
                    url:'https://github.com/phaniv2107/Jenkins-Zero-To-Hero.git'
                }
            }
        }
        stage ('stag4 from sharedlibrary'){
            steps {
              shared-libraries.firstlib()
            }
        }

    }
}
